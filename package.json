{
  "name": "fable-repl",
  "version": "1.0.0",
  "description": "Fable REPL",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "build-fcs": "cd src && dotnet fable npm-rollup --port free --verbose",
    "watch-fsc": "cd src && dotnet fable npm-rollup --port free --verbose -- -w",
    "build-app": "cd src && dotnet fable webpack",
    "watch-app": "cd src && dotnet fable webpack -- -w",
    "start-app": "cd src && dotnet fable webpack-dev-server --define DEBUG",
    "webpack": "node node_modules/webpack/bin/webpack.js",
    "rollup": "node node_modules/rollup/bin/rollup -c",
    "start": "http-server -c-1 ./public -p 8888 -o"
  },
  "dependencies": {
    "bulma": "^0.6.1",
    "font-awesome": "^4.7.0",
    "monaco-editor": "^0.10.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "recharts": "^1.0.0-apha.5",
    "requirejs": "^2.3.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-standalone": "^6.26.0",
    "babel-template": "^6.26.0",
    "css-loader": "^0.28.7",
    "fable-loader": "^1.0.7",
    "file-loader": "^1.1.5",
    "http-server": "^0.10.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.6.1",
    "postcss-loader": "^2.0.9",
    "remotedev": "^0.2.7",
    "rollup": "^0.49.2",
    "rollup-plugin-fable": "^1.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.8.2"
  },
  "browserslist": [
    "last 2 versions"
  ]
}